<AnchorPane fx:id="mainPane" prefHeight="700" prefWidth="700" stylesheets="@CartCss.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.CustomerPage.CartController">
