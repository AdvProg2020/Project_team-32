<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ManagerPage.ProductManagerPageController">

    <children>
        <TableView fx:id="productsTable" layoutX="74.0" layoutY="121.0" prefHeight="525.0" prefWidth="551.0" stylesheets="@tableview-style.css">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="107.0" text="Id" />
                <TableColumn fx:id="nameColumn" prefWidth="115.0" text="name" />
                <TableColumn fx:id="categoryColumn" prefWidth="181.0" text="category" />
                <TableColumn fx:id="statusColumn" prefWidth="145.0" text="status" />
            </columns>
        </TableView>
        <Text fx:id="welcomeText" fill="#ddab21" layoutX="74.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Product manager">
            <font>
                <Font name="Freestyle Script" size="43.0" />
            </font>
        </Text>
        <Button fx:id="removeButton" layoutX="482.0" layoutY="37.0" mnemonicParsing="false" onAction="#remove" prefHeight="70.0" prefWidth="70.0" stylesheets="@information-page-style.css" />
    </children>

</AnchorPane>
